module out_num_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [7:0] num;
reg reset;
// wires                                               
wire [6:0] lsd;
wire [6:0] msd;

wire sampler;                             

// assign statements (if any)                          
out_num i1 (
// port map - connection between master ports and signals/registers   
	.lsd(lsd),
	.msd(msd),
	.num(num),
	.reset(reset)
);
// num[ 7 ]
initial
begin
	num[7] = 1'b0;
end 
// num[ 6 ]
initial
begin
	num[6] = 1'b0;
end 
// num[ 5 ]
initial
begin
	num[5] = 1'b0;
end 
// num[ 4 ]
initial
