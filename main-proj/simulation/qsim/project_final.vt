	num[3] = 1'b0;
end 
// num[ 2 ]
initial
begin
	num[2] = 1'b1;
end 
// num[ 1 ]
initial
begin
	num[1] = 1'b0;
end 
// num[ 0 ]
initial
begin
	num[0] = 1'b1;
end 

// reset
initial
begin
	reset = 1'b0;
end 

out_num_vlg_sample_tst tb_sample (
	.num(num),
	.reset(reset),
	.sampler_tx(sampler)
);

out_num_vlg_check_tst tb_out(
	.lsd(lsd),
	.msd(msd),
	.sampler_rx(sampler)
);
endmodule

